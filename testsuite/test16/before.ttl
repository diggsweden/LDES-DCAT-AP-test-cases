@prefix dcat: <http://www.w3.org/ns/dcat#> .
@prefix dcterms: <http://purl.org/dc/terms/> .
@prefix foaf: <http://xmlns.com/foaf/0.1/> .
@prefix ex: <http://example.com/> .

ex:a a dcat:Catalog ;
    dcterms:title "Catalog A" ;
    dcat:dataset ex:b ;
    dcat:service ex:c .

ex:b a dcat:Dataset ;
    dcterms:title "Dataset B" ;
    dcterms:description "A DCAT-AP dataset." .

ex:c a dcat:DataService ;
    dcterms:title "Data Service C" ;
    dcat:endpointURL <https://example.com/c/access> ;
    dcat:servesDataset ex:b .
