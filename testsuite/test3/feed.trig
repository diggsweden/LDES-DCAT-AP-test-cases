@prefix ldes: <https://w3id.org/ldes#> .
@prefix tree: <https://w3id.org/tree#> .
@prefix dcterms: <http://purl.org/dc/terms/> .
@prefix as: <https://www.w3.org/ns/activitystreams#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix dcat: <http://www.w3.org/ns/dcat#> .
@prefix ex: <http://example.com/> .

<#Feed> a ldes:EventStream ;
    tree:shape <https://semiceu.github.io/LDES-DCAT-AP-feeds/shape.ttl#ActivityShape> ;
    dcterms:title "Adding a dataset to an existing catalog" ;
    ldes:timestampPath as:published ;
    ldes:versionOfPath as:object ;
    tree:view <> ;
    tree:member <https://example.com/b#Event1>, <https://example.com/a#Event1> .

<https://example.com/b#Event1> a as:Create ;
    as:object ex:b ;
    as:published "2023-10-01T12:00:00Z"^^xsd:dateTime .

<https://example.com/b#Event1> {
    ex:b a dcat:Dataset ;
        dcterms:title "Dataset B" ;
        dcterms:description "A DCAT-AP dataset." .
}

<https://example.com/a#Event1> a as:Update ;
    as:object ex:a ;
    as:published "2023-10-01T12:01:00Z"^^xsd:dateTime .

<https://example.com/a#Event1> {
    ex:a a dcat:Catalog ;
        dcterms:title "Catalog A" ;
        dcterms:description "A DCAT-AP catalog." ;
        dcterms:publisher ex:pub ;
        dcat:dataset ex:b .
}