PREFIX dcat: <http://www.w3.org/ns/dcat#>
PREFIX dcterms: <http://purl.org/dc/terms/>
PREFIX ex: <http://example.com/>
PREFIX rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>

ASK {
  ex:a a dcat:Catalog ;
    dcat:service ex:c .
  ex:c a dcat:DataService ;
    dcterms:title "Data Service C" ;
    dcat:endpointURL <https://example.com/c/access> ;
    dcat:servesDataset ex:b .
  OPTIONAL {
    ex:b a ?o .
  }
  FILTER ( !bound(?o) )
}